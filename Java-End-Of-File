import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
              
        Scanner scan = new Scanner(System.in);
        int linenumber = 1;
        
        while (scan.hasNext()){
            String linecontent = scan.nextLine();
            System.out.println(linenumber + " " + linecontent);
            linenumber++;
        }
        
        scan.close();
        
    }
}
